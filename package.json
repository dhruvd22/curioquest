{
  "name": "curioquest",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "validate": "tsx scripts/validate-content.ts",
    "audit": "tsx scripts/audit-content.ts",
    "generate": "tsx scripts/generate/run-batch.ts",
    "generate:review": "tsx scripts/generate/run-batch.ts --all --images=skip --review-mode",
    "review:diff": "tsx scripts/review/make-diff.ts --all",
    "approve": "tsx scripts/review/approve.ts",
    "publish": "tsx scripts/review/publish.ts --slug",
    "cleanup": "tsx scripts/cleanup-content.ts",
    "runpod": "node serverless.mjs"
  },
  "dependencies": {
    "express": "^5.1.0",
    "mri": "^1.2.0",
    "next": "14.2.5",
    "openai": "^5.19.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "serverless-http": "^4.0.0",
    "sharp": "^0.33.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4"
  }
}
